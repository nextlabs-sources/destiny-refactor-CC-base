<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="com.bluejungle.destiny.container.shared.policydeploymentmgr.hibernateimpl">
	
	<class name="AgentProcessorDO" table="COMPONENT" dynamic-insert="false" dynamic-update="false">

		<!-- Generic persisted properties -->
		<id name="id">
			<generator class="native"/>
		</id>

		<!-- Object-specific properties -->
		<property name="name" type="string" not-null="true" unique="true" length="128"/>
	</class>
</hibernate-mapping>
