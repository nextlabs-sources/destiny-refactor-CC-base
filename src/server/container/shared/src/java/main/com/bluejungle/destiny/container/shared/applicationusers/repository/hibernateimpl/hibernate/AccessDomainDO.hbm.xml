<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping package="com.bluejungle.destiny.container.shared.applicationusers.repository.hibernateimpl">

	<class name="AccessDomainDO"  table="APPLICATION_USER_DOMAIN">

		<id name="id">
			<generator class="native"/>
		</id>

        <property name="name" type="string" length="255" not-null="true" unique="true"/>

	</class>

</hibernate-mapping>
