<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping package="com.bluejungle.destiny.container.shared.agentmgr.hibernateimpl">
	
	<class name="HeartbeatRecordDO" table="HEART_BEAT_RECORD">

		<!-- Generic persisted properties -->
		<id name="id">
			<generator class="native"/>
		</id>
		
		<!-- Object-specific properties -->
		<property name="timestamp" type="com.bluejungle.framework.datastore.hibernate.usertypes.CalendarToLongUserType"/>
	</class>
</hibernate-mapping>
